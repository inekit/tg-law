{
  "scripts": {
    "start:prod": "node ./src",
    "migration:run": "typeorm migration:run --dataSource ./migrations/data-source.js",
    "migration:generate": "typeorm migration:generate --dataSource ./migrations/data-source.js --outputJs ./migrations/migrations/mg1",
    "start:dev": "node --require dotenv/config ./src",
    "typeorm:dev": "node --require ts-node/register --require dotenv/config ./node_modules/typeorm/cli.js",
    "migration:dev:run": "npm run typeorm:dev -- --dataSource ./migrations/data-source.js migration:run",
    "migration:dev:generate": "npm run typeorm:dev -- --dataSource ./migrations/data-source.js --outputJs ./migrations/migrations/mg1 migration:generate"
  },
  "moduleResolution": "node",
  "name": "tg-subscription",
